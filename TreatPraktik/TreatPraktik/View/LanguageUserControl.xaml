<UserControl x:Class="TreatPraktik.View.LanguageUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="40" Width="130.4">
    <Grid>
        <Button HorizontalAlignment="Left" VerticalAlignment="Top" Height="40" Width="60" BorderThickness="0" Click="Button_Click_UK">
            <Button.Background>
                <ImageBrush ImageSource="/Ressources/Uk_icon.png" />
            </Button.Background>
        </Button>
        <Button HorizontalAlignment="Left" VerticalAlignment="Top" Height="40" Width="60" BorderThickness="0" Margin="70,0,0,0" Click="Button_Click_DK">
            <Button.Background>
                <ImageBrush ImageSource="/Ressources/Dk-icon.png" />
            </Button.Background>
        </Button>


    </Grid>
</UserControl>
